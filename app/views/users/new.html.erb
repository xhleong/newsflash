New User Form Page

<%= form_for @user do |f| %>
  <p>
  <%= f.label :name %>
  <%= f.text_field :name, required: 'required' %>
  </p>

  <p>
  <%= f.label :email %>
  <%= f.text_field :email, required: 'required' %>
  </p>

  <p>
    <%= f.label :password %>
    <%=f.password_field :password %>
  </p>

  <p>
    <%= f.submit %>
  </p>

<% end %>

